{
	"schemaVersion": 1,
	"id": "notify",
	"version": "${version}",
	"name": "Notify",
	"description": "This is a mod that is used to make Dynamic Notes and To-Do lists!",
	"authors": [
		"Benjammin Gerresheim & Liam Bruno"
	],
	"contact": {
		"homepage": "https://benjishomelab.ddns.net/",
		"sources": "https://github.com/McBenjihood/notify"
	},
	"license": "CC0-1.0",
	"icon": "assets/notify/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.notify.Notify"
		],
		"client": [
			"com.notify.NotifyClient"
		]
	},
	"mixins": [
		"notify.mixins.json",
		{
			"config": "notify.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}